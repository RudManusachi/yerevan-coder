{
  "name": "yerevan_coder",
  "version": "0.0.1",
  "description": "The code behind yerevancoder.com",
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "moments": "0.0.2",
    "react": "^15.3.2",
    "react-big-calendar": "^0.10.3",
    "react-dom": "^15.3.2",
    "react-modal": "^1.5.2"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-3"
    ],
    "plugins": [
      "transform-es2015-modules-commonjs",
      "transform-async-to-generator",
      "transform-runtime"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-plugin-transform-async-to-generator": "6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-3": "^6.11.0",
    "webpack": "^1.13.2"
  },
  "scripts": {
    "compile_front_end": "mkdir -p dist;node_modules/babel-cli/bin/babel.js lib --out-dir dist",
    "bundle_front_end": "node_modules/webpack/bin/webpack.js --progress --colors dist/homepage.js public/bundle.js",
    "front_end": "npm run compile_front_end && npm run bundle_front_end"
  },
  "author": "Edgar Aroutiounian <edgar.factorial@gmail.com>",
  "license": "MIT"
}
